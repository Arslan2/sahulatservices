<script>

    $('#carouselExampleProduct').on('slide.bs.carousel', function (e) {


        var $e = $(e.relatedTarget);
        var idx = $e.index();
        var itemsPerSlide = 3;
        var totalItems = $('#carouselExampleProduct .sliderProduct').length;

        if (idx >= totalItems - (itemsPerSlide - 1)) {
            var it = itemsPerSlide - (totalItems - idx);
            for (var i = 0; i < it; i++) {
                // append slides to end
                if (e.direction == "left") {
                    $('#carouselExampleProduct .sliderProduct').eq(i).appendTo('.carous-inner');
                } else {
                    $('#carouselExampleProduct .sliderProduct').eq(0).appendTo('.carous-inner');
                }
            }
        }
    });
</script> 

