            function loadCodeAddress() {
                if(document.getElementById('addressMap') !== null){
                  var address = document.getElementById('addressMap').value;  
                }
                if(document.getElementById('lat') !== null){
                  var lati = document.getElementById('lat').value;  
                }
                
                if(document.getElementById('lng') !== null){
                 var lngi = document.getElementById('lng').value;   
                }
                
                if(lati != null && lngi != null){
                  var latlng = new google.maps.LatLng(lati, lngi);  
                }
                if(map != undefined){
                  map.setCenter(latlng);  
                }
                
                if (marker)
                    marker.setMap(null);
                marker = new google.maps.Marker({
                    map: map,
                    position: latlng,
                    draggable: true
                });
                google.maps.event.addListener(marker, "dragend", function () {
                    document.getElementById('lat').value = marker.getPosition().lat();
                    document.getElementById('lng').value = marker.getPosition().lng();
                    currentAddress();
                });
