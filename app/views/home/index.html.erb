                 }
                 });*/
            }
            function codeAddress() {
                var address = document.getElementById('addressMap').value;
                geocoder.geocode({'address': address}, function (results, status) {
                    if (status == google.maps.GeocoderStatus.OK) {
                        map.setCenter(results[0].geometry.location);
                        document.getElementById('lat').value = results[0].geometry.location.lat();
                        document.getElementById('lng').value = results[0].geometry.location.lng();
                        var geolocation = results[0].geometry.location.lat() + "," + results[0].geometry.location.lng();
                        var jsonReq = "https://maps.googleapis.com/maps/api/geocode/json?latlng=" + geolocation + "&key=AIzaSyBec81FMXTkbibE4uaIDBBs3C_jYFRF0EM";
                        $.getJSON(jsonReq, showAddress);
                        if (marker)
                            marker.setMap(null);
                        marker = new google.maps.Marker({
                            map: map,
                            position: results[0].geometry.location,
                            draggable: true
                        });
                        google.maps.event.addListener(marker, "dragend", function () {
                            document.getElementById('lat').value = marker.getPosition().lat();
                            document.getElementById('lng').value = marker.getPosition().lng();
                            currentAddress();
                        });
                    }
                });
            }
        </script>
